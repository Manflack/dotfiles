<application>
  <component name="GetterTemplates" defaultTemplateName="Without get prefix">
    <option name="templates">
      <list>
        <TemplateResource>
          <option name="fileName" value="Without get prefix" />
          <option name="template" value="#if($field.modifierStatic)&#10;static ##&#10;#end&#10;$field.type ##&#10;#if($field.recordComponent)&#10;    ${field.name}##&#10;#else&#10;    #set($name = $StringUtil.capitalizeWithJavaBeanConvention($StringUtil.sanitizeJavaIdentifier($helper.getPropertyName($field, $project))))&#10;    #if ($field.boolean &amp;&amp; $field.primitive)&#10;      is${name}##&#10;    #else&#10;      ${field.name}##&#10;    #end&#10;    ##&#10;#end&#10;() {&#10;return $field.name;&#10;}" />
        </TemplateResource>
      </list>
    </option>
  </component>
</application>